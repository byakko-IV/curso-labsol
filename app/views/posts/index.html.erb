<table border="1">
  <thead>
    <tr>
        <td>title</td>
        <td>body</td>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td>
          <%= post.title %>
        </td>
        <td>
          <%= post.body %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>


